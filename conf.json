{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": [],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles": true
    }
  },
  "opts": {
    "template": "node_modules/docdash",
    "destination": "./doc/"
  },
  "docdash": {
    "meta": { "title": "mind-elixir" },
    "menu": {
      "github": {
        "href": "https://github.com/ssshooter/mind-elixir-core",
        "target": "_blank",
        "class": "menu-item"
      }
    }
  }
}
