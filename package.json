{
  "name": "mind-elixir",
  "version": "0.18.0",
  "description": "Mind elixir is a free open source mind map core.",
  "main": "dist/MindElixir.js",
  "scripts": {
    "start": "webpack-dev-server --mode development --env.dist=0",
    "startd": "webpack-dev-server --mode development --env.dist=1",
    "test": "jest --runInBand",
    "build": "webpack --mode production",
    "doc": "./node_modules/.bin/jsdoc src/ -R readme.md -c conf.json",
    "md": "./node_modules/.bin/jsdoc2md 'src/*.js' > api.md",
    "publish2": "npm run build && npm version patch -m \"build: release %s\" && npm publish",
    "beta": "npm run build && npm publish --tag beta"
  },
  "files": [
    "package.json",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ssshooter/mind-elixir-core"
  },
  "homepage": "https://mindelixir.ink/",
  "author": "ssshooter",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.11.5",
    "@babel/runtime": "^7.15.4",
    "@typescript-eslint/parser": "^5.0.0",
    "babel-loader": "^8.0.6",
    "canvg": "^3.0.6",
    "css-loader": "^2.1.1",
    "docdash": "^1.1.1",
    "eslint": "^8.0.0",
    "file-loader": "^4.0.0",
    "html-webpack-plugin": "^5.3.2",
    "jest": "^26.4.2",
    "jsdoc": "^3.6.3",
    "jsdoc-to-markdown": "^5.0.0",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "puppeteer": "^5.2.1",
    "puppeteer-to-istanbul": "^1.4.0",
    "regenerator-runtime": "^0.13.7",
    "style-loader": "^0.23.1",
    "ts-loader": "^9.2.6",
    "typescript": "^4.4.3",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "url-loader": "^2.0.0",
    "webpack": "5.58.1",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.5.1"
  }
}
